IN MADDM

import model DMSimp_t-S3M_3rd
define darkmatter xm
define coannihilator b2
generate relic_density
output modelxm-b2
launch modelxm-b2
set mxm 500
set mmys3qu3 510
set lams3d3x3 0.17
set lams3q3x3 0
set lams3u3x3 0


IN MADGRAPH
import model DMSimp_t-S3M_3rd
define new = xm b2 b2~
generate p p > b2 b2~
output modelxm-b2-madgraph
launch modelxm-b2-madgraph
set mxm 500
set mys3d3 510
set wys3d3 auto
set lams3d3x3 0.17
set lams3q3x3 0
set lams3u3x3 0

